{
  "name": "helm_core_gpt4o",
  "model": {
    "provider": "openai",
    "name": "gpt-4o"
  },
  "scenarios": [
    {
      "scenario": "mmlu",
      "max_examples": 100
    },
    {
      "scenario": "hellaswag",
      "max_examples": 100
    }
  ],
  "adapters": {
    "temperature": 0.0,
    "max_tokens": 512,
    "top_p": 1.0
  },
  "metrics": ["exact_match", "perplexity", "toxicity"],
  "output_path": "outputs/helm/core_gpt4o"
}
